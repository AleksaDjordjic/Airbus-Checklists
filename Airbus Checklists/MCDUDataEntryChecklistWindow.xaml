            </CheckBox.LayoutTransform>
        </CheckBox>
        <Rectangle Fill="White" Height="30" Margin="10,515,10,0" Stroke="Black" VerticalAlignment="Top"/>
        <Rectangle Fill="White" Height="30" Margin="10,545,10,0" Stroke="Black" VerticalAlignment="Top"/>
        <Rectangle Fill="White" Height="30" Margin="10,575,10,0" Stroke="Black" VerticalAlignment="Top"/>
        <Rectangle Fill="White" Height="30" Margin="10,605,10,0" Stroke="Black" VerticalAlignment="Top"/>
        <Rectangle Fill="White" Height="30" Margin="10,635,10,0" Stroke="Black" VerticalAlignment="Top"/>
        <Rectangle Fill="White" Height="30" Margin="10,665,10,0" Stroke="Black" VerticalAlignment="Top"/>
        <Rectangle Fill="White" Height="30" Margin="10,695,10,0" Stroke="Black" VerticalAlignment="Top"/>
        <TextBlock HorizontalAlignment="Left" Margin="17,485,0,0" TextWrapping="Wrap" Text="RAD/NAV Page" VerticalAlignment="Top" Height="30" Width="522" FontFamily="Impact" FontSize="24" FontWeight="Bold" TextDecorations="Underline"/>
        <TextBlock HorizontalAlignment="Left" Margin="17,545,0,0" TextWrapping="Wrap" Text="INIT Page 2" VerticalAlignment="Top" Height="30" Width="522" FontFamily="Impact" FontSize="24" FontWeight="Bold" TextDecorations="Underline"/>
        <TextBlock HorizontalAlignment="Left" Margin="17,605,0,0" TextWrapping="Wrap" Text="PERF Page" VerticalAlignment="Top" Height="30" Width="522" FontFamily="Impact" FontSize="24" FontWeight="Bold" TextDecorations="Underline"/>
        <CheckBox x:Name="ChecklistItem13" Content="Enter Radio Navigation Info (if needed)" Height="15" Margin="13,515,0,0" VerticalAlignment="Top" FontSize="11" Click="ChecklistItem_Click" HorizontalAlignment="Left" Width="262">
            <CheckBox.LayoutTransform>
                <ScaleTransform ScaleX="2" ScaleY="2" />
            </CheckBox.LayoutTransform>
        </CheckBox>
        <CheckBox x:Name="ChecklistItem14" Content="Enter Block Fuel (Total Fuel)" Height="15" Margin="13,575,0,0" VerticalAlignment="Top" FontSize="11" Click="ChecklistItem_Click" HorizontalAlignment="Left" Width="262">
            <CheckBox.LayoutTransform>
                <ScaleTransform ScaleX="2" ScaleY="2" />
            </CheckBox.LayoutTransform>
        </CheckBox>
        <CheckBox x:Name="ChecklistItem15" Content="Enter Takeoff Flaps Setting" Height="15" Margin="13,635,0,0" VerticalAlignment="Top" FontSize="11" Click="ChecklistItem_Click" HorizontalAlignment="Left" Width="262">
            <CheckBox.LayoutTransform>
                <ScaleTransform ScaleX="2" ScaleY="2" />
            </CheckBox.LayoutTransform>
        </CheckBox>
        <CheckBox x:Name="ChecklistItem16" Content="Check V1, VR + V2 Speeds" Height="15" Margin="13,665,0,0" VerticalAlignment="Top" FontSize="11" Click="ChecklistItem_Click" HorizontalAlignment="Left" Width="262">
            <CheckBox.LayoutTransform>
                <ScaleTransform ScaleX="2" ScaleY="2" />
            </CheckBox.LayoutTransform>
        </CheckBox>
        <CheckBox x:Name="ChecklistItem17" Content="Check Transition Altitude" Height="15" Margin="13,695,0,0" VerticalAlignment="Top" FontSize="11" Click="ChecklistItem_Click" HorizontalAlignment="Left" Width="262">
            <CheckBox.LayoutTransform>
                <ScaleTransform ScaleX="2" ScaleY="2" />
            </CheckBox.LayoutTransform>
        </CheckBox>
    </Grid>
</Window>