            <CheckBox.LayoutTransform>
                <ScaleTransform ScaleX="2" ScaleY="2" />
            </CheckBox.LayoutTransform>
        </CheckBox>
        <CheckBox x:Name="ChecklistItem8" Content="Enter Airways + Waypoints" Height="15" Margin="13,335,0,0" VerticalAlignment="Top" FontSize="11" Click="ChecklistItem_Click" HorizontalAlignment="Left" Width="262">
            <CheckBox.LayoutTransform>
                <ScaleTransform ScaleX="2" ScaleY="2" />
            </CheckBox.LayoutTransform>
        </CheckBox>
        <CheckBox x:Name="ChecklistItem9" Content="Select Arrival Runway" Height="15" Margin="13,365,0,0" VerticalAlignment="Top" FontSize="11" Click="ChecklistItem_Click" HorizontalAlignment="Left" Width="262">
            <CheckBox.LayoutTransform>
                <ScaleTransform ScaleX="2" ScaleY="2" />
            </CheckBox.LayoutTransform>
        </CheckBox>
        <CheckBox x:Name="ChecklistItem10" Content="Select Arrival Procedure" Height="15" Margin="13,395,0,0" VerticalAlignment="Top" FontSize="11" Click="ChecklistItem_Click" HorizontalAlignment="Left" Width="262">
            <CheckBox.LayoutTransform>
                <ScaleTransform ScaleX="2" ScaleY="2" />
            </CheckBox.LayoutTransform>
        </CheckBox>
        <CheckBox x:Name="ChecklistItem11" Content="Check And Remove Discontinuities" Height="15" Margin="13,425,0,0" VerticalAlignment="Top" FontSize="11" Click="ChecklistItem_Click" HorizontalAlignment="Left" Width="262">
            <CheckBox.LayoutTransform>
                <ScaleTransform ScaleX="2" ScaleY="2" />
            </CheckBox.LayoutTransform>
        </CheckBox>
        <TextBlock Margin="17,65,0,0" TextWrapping="Wrap" Text="INIT Page 1" VerticalAlignment="Top" Height="30" FontFamily="Impact" FontSize="24" FontWeight="Bold" TextDecorations="Underline" HorizontalAlignment="Left" Width="522"/>
        <TextBlock HorizontalAlignment="Left" Margin="17,245,0,0" TextWrapping="Wrap" Text="F-PLN Page" VerticalAlignment="Top" Height="30" Width="522" FontFamily="Impact" FontSize="24" FontWeight="Bold" TextDecorations="Underline"/>
        <Rectangle Fill="White" Height="30" Margin="10,455,10,0" Stroke="Black" VerticalAlignment="Top"/>
        <Rectangle Fill="White" Height="30" Margin="10,485,10,0" Stroke="Black" VerticalAlignment="Top"/>
